<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<adtf:project xmlns:adtf="adtf">
 <general_settings>
  <property name="active_configuration" type="String" value="Main" />
  <property name="enable_messagebus" type="Bool" value="true" />
  <property name="synchronizing_type" type="Int" value="1" />
  <property name="global_log_level" type="Int" value="3" />
  <property name="media_description_files" type="String" value="./../description/*.description;./../addons/*/description/*.description;./../src/examples/src/description/*.description;./../../../description/*.*;./../../../../description/*.description;~/AADC/description/*.description" />
 </general_settings>
 <configurations>
  <configuration description="" name="Main">
   <graph>
    <subgraph alias="Base_Config" idref="Base_Config" priority="5" />
    <connection connection_priority="50" dest="Base_Config.Watchdog_Alive_Flag" interface="" source="AADC_Watchdog_Trigger.WatchdogAliveSignal" />
    <connection connection_priority="50" dest="AADC_State_Controller.Jury_Struct" interface="" source="Base_Config.Jury_Struct" />
    <connection connection_priority="50" dest="Base_Config.Driver_Struct" interface="" source="AADC_State_Controller.Driver_Struct" />
    <filter alias="AADC_Watchdog_Trigger" idref="adtf.aadc.watchdogTrigger" priority="6">
     <settings />
    </filter>
    <filter alias="AADC_State_Controller" idref="adtf.aadc.aadc_stateController" priority="4">
     <settings>
      <property name="Maneuver File" type="String" value="../files/Maneuverlist.xml" />
     </settings>
    </filter>
    <filter alias="AADC_Xtion_Camera" idref="adtf.aadc.aadc_xtionCamera" priority="5">
     <settings>
      <property name="Configuration File" type="String" value="./../../../../configuration_files/xtionSettings.xml" />
     </settings>
    </filter>
   </graph>
  </configuration>
 </configurations>
 <fileinfo adtfversion="2.13.0" configversion="1.3" />
 <configuration_settings />
 <adtf_include source="$(HOME)/AADC/config/BaseConfig/IncludeConfig.xml" />
 <plugins>
  <plugin optional="true" url="$(HOME)/AADC/bin/Linux/gcc/$(BUILD_TYPE_2)/*.plb" />
  <!--<plugin optional="true" url="$(HOME)/AADC/bin/Windows/vc10/$(BUILD_TYPE_2)/*.plb" /> -->
 </plugins>
</adtf:project>
