			</SCM-ActivityLevel>
		</SCM-Step>
		<SCM-Step id="1">
			<SCM-ActivityLevel>
				<SCM-Active>
					<SCM-FilterID id="21"> 
						<SCM-Action enable="true" start="true" command="21050"/> <!-- AC_SA_STOP_CAR = 8010 -->
					</SCM-FilterID>
				</SCM-Active>
